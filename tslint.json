{
  "extends": "tslint-config-airbnb",
  "rules": {
    "ter-arrow-parens": false,
    "function-name": false,
    "variable-name": false,
    "trailing-comma": false,
    "import-name": false,
    "no-increment-decrement": false,
    "no-param-reassign": false,
    "max-line-length": {
      "options": [120]
    },
    "align": [true, "parameters", "statements"]
  },
  "jsRules": {
  }
}
