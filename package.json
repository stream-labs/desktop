{
  "name": "slobs-client",
  "description": "Streamlabs streaming software",
  "author": "General Workings, Inc.",
  "license": "GPL-3.0",
  "version": "0.6.7",
  "main": "main.js",
  "scripts": {
    "compile": "yarn clear && webpack --progress",
    "ci:compile": "yarn clear && webpack",
    "watch": "yarn clear && webpack --watch --progress",
    "start": "electron .",
    "install-node-obs": "node bin/install-node-obs.js && yarn install-plugins",
    "rebuild-node-obs": "cd ../node-obs/ && cmake-js build --runtime electron --runtime-version 1.7.7 && cmake --build build --target install --config Release && rm -rf ../streamlabs-obs/node-obs/ && cp -r ./build/distribute/node-obs/ ../streamlabs-obs/ && cd ../streamlabs-obs && node_modules/7zip-bin-win/x64/7za.exe x plugins/obs-browser-2987-old.7z -onode-obs/",
    "install-plugins": "node_modules/7zip-bin-win/x64/7za.exe x plugins/obs-browser-2987-old.7z -onode-obs/",
    "package": "rm -rf dist && build -w --x64 --em.env=production",
    "deploy": "node bin/deploy.js",
    "test": "ava",
    "clear": "rm -rf bundles/media"
  },
  "build": {
    "appId": "com.streamlabs.slobs",
    "productName": "Streamlabs OBS",
    "icon": "media/images/icon.ico",
    "files": [
      "bundles",
      "node_modules",
      "!node_modules/obs-studio-node/obs-studio",
      "vendor",
      "updater/index.html",
      "updater/Updater.js",
      "index.html",
      "main.js",
      "obs-api"
    ],
    "extraFiles": [
      "node-obs"
    ],
    "asarUnpack": [
      "**/*.node"
    ],
    "publish": {
      "provider": "generic",
      "url": "https://d1g6eog1uhe0xm.cloudfront.net"
    }
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "serial": true
  },
  "cmake-js": {
    "runtime": "electron",
    "runtimeVersion": "1.7.7",
    "arch": "x64"
  },
  "dependencies": {
    "archiver": "^2.0.3",
    "asar": "^0.13.0",
    "aws-sdk": "^2.43.0",
    "backtrace-node": "^0.7.3",
    "electron-updater": "^1.14.0",
    "font-manager": "^0.2.2",
    "lodash": "^4.17.4",
    "node-libuiohook": "file:./node-libuiohook",
    "obs-studio-node": "file:./obs-studio-node",
    "rimraf": "^2.6.1",
    "uuid": "^3.0.1",
    "vue-popperjs": "^1.2.2"
  },
  "devDependencies": {
    "7zip-bin": "^2.0.4",
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.18",
    "ava": "^0.19.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "circular-dependency-plugin": "^3.0.0",
    "css-loader": "^0.26.1",
    "devtron": "^1.4.0",
    "electron": "1.7.7",
    "electron-builder": "^19.22.1",
    "electron-devtools-installer": "^2.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-vue": "^2.0.1",
    "file-loader": "^0.11.2",
    "js-yaml": "^3.8.3",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "lodash-decorators": "^4.3.1",
    "moment": "^2.17.1",
    "progress": "^2.0.0",
    "rxjs": "^5.4.3",
    "spectron": "^3.6.4",
    "style-loader": "^0.13.1",
    "traverse": "^0.6.6",
    "ts-loader": "^2.3.2",
    "tslint": "^5.4.2",
    "tslint-config-airbnb": "^5.0.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.4.2",
    "urijs": "^1.18.5",
    "vue": "^2.1.10",
    "vue-color": "^2.0.6",
    "vue-loader": "^10.2.0",
    "vue-multiselect": "2.0.3",
    "vue-property-decorator": "^5.0.1",
    "vue-slider-component": "^2.2.7",
    "vue-template-compiler": "^2.1.10",
    "vuedraggable": "^2.8.5",
    "vuex": "^2.1.1",
    "webpack": "^2.2.1"
  }
}
